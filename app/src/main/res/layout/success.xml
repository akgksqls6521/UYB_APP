<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rela"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/pcbg"
    tools:context="comakgksqls6521.httpsgithub.uyb_mapsi.MainActivity">

    <ImageView
        android:id="@+id/copyright"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="120dp"
        android:src="@drawable/cr"
        android:background="@android:color/transparent"/>
    <ImageView
        android:id="@+id/maneking"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/doll"
        android:background="@android:color/transparent"/>
    <ImageView
        android:id="@+id/cth_pants"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent"/>
    <ImageView
        android:id="@+id/cth_top"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent"/>
    <ImageButton
        android:id="@+id/share"
        android:layout_marginTop="1080dp"
        android:layout_marginLeft="60dp"
        android:src="@drawable/facebookbtn"
        android:scaleX="0.7"
        android:scaleY="0.7"
        android:background="@android:color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="공유"/>
    <ImageButton
        android:layout_marginLeft="250dp"
        android:layout_marginTop="20dp"
        android:scaleX="0.7"
        android:scaleY="0.7"
        android:src="@drawable/gallerysavebtn"
        android:background="@android:color/transparent"
        android:id="@+id/save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="저장"/>
    <ImageButton
        android:layout_marginTop="1080dp"
        android:layout_marginLeft="440dp"
        android:src="@drawable/closetsavebtn"
        android:background="@android:color/transparent"
        android:id="@+id/clo"
        android:scaleX="0.7"
        android:scaleY="0.7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="옷장"/>

</RelativeLayout>
